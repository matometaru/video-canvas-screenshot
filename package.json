{
  "name": "video-canvas-screenshot",
  "version": "1.0.1",
  "description": "Take a screenshot of the video and canvas.",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.mjs",
  "types": "./dist/types/index.d.ts",
  "files": ["dist"],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.umd.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matometaru/video-canvas-screenshot.git"
  },
  "engines": {
    "node": ">= 16"
  },
  "scripts": {
    "dev": "npm run dev -w examples",
    "build:examples": "npm run build -w examples",
    "build": "tsc && vite build",
    "prepare": "npm run build"
  },
  "keywords": [
    "video",
    "canvas",
    "screenshot"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "typescript": "^5.2.2",
    "vite": "^4.5.0"
  },
  "workspaces": [
    "examples"
  ]
}
